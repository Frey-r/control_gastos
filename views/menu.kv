#:kivy 2.0.0
<ui>:
    MDScreen:
        name: "menu_principal"
        MDBoxLayout:
            orientation: "vertical"
            md_bg_color: app.theme_cls.primary_color
            MDBoxLayout: 
                size_hint: 1, 1
                orientation: "vertical"
                padding: '5dp'
                margin: '5dp'
                MDCard:
                    name: "Barcode"
                    margin: '5dp'
                    on_press:
                        app.working()
                MDCard:
                    name: "Ingreso Gastos"
                    margin: '5dp'
                    on_press: 
                        app.read_barcode()  
                    Label:
                        text: "Nueva Compra"
                        color: app.theme_cls.accent_color

                MDCard:
                    name: "Estadisticas"
                    margin: '5dp'
                    on_press:
                        app.working()
                MDCard:
                    name: "Grupo"
                    margin: '5dp'
                    on_press:
                        app.working()
                MDCard:
                    name: "Distribuidores"
                    margin: '5dp'
                    on_press:
                        app.working() 